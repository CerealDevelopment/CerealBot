{
  "name": "cerealbot",
  "version": "0.0.3",
  "description": "Cereal Bot for extreme Cereal Content",
  "main": "lib/index.js",
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "build": "babel src -d lib --extensions \".ts\"",
    "start": "node --experimental-json-modules ./lib/index.js",
    "prepare": "husky install",
    "pretty": "npx prettier --write \"./src\" \"./tests\"",
    "clean": "rm -rf node_modules",
    "clean-install": "npm run-script clean && npm install",
    "docker-pull": "docker cerealdevelopment/cerealbot:latest",
    "docker-build": "docker build --tag cerealdevelopment/cerealbot:latest .",
    "docker-push": "docker push cerealdevelopment/cerealbot:latest",
    "docker-run": "docker run -d -v $(pwd)/config.json:/opt/cerealbot/config.json -e BOT_TOKEN=$BOT_TOKEN cerealdevelopment/cerealbot:latest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CerealDevelopment/CerealBot.git"
  },
  "keywords": [
    "Cereal",
    "Bot",
    "Discord"
  ],
  "author": "Cereal Development",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/CerealDevelopment/CerealBot/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged --ignore-path=.gitignore"
    }
  },
  "homepage": "https://github.com/CerealDevelopment/CerealBot#readme",
  "dependencies": {
    "@discordjs/opus": "^0.4.0",
    "discord.js": "^12.5.1",
    "node-fetch": "^2.6.1",
    "require": "^2.4.20"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.0",
    "@babel/core": "^7.13.8",
    "@babel/node": "^7.13.0",
    "@babel/preset-env": "^7.13.8",
    "@babel/preset-typescript": "^7.13.0",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.37",
    "@types/node-fetch": "^2.5.10",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "prettier": "2.2.1",
    "pretty-quick": "^3.1.0",
    "typescript": "^4.2.2"
  }
}
