{
  "name": "cerealbot",
  "version": "0.0.9",
  "description": "Cereal Bot for extreme Cereal Content",
  "main": "lib/index.js",
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "build": "npm run-script pretty && babel src -d lib --extensions \".ts\"",
    "start": "node ./lib/index.js",
    "build-and-start": "npm run-script build && npm run-script start",
    "pretty": "npx prettier --print-width 120 --arrow-parens avoid --write \"./src\" \"./tests\"",
    "clean": "rm -rf node_modules lib",
    "clean-install": "npm run-script clean && npm install",
    "clean-logs": "rm logs/*",
    "docker-pull": "docker cerealdevelopment/cerealbot:latest",
    "docker-build": "npm run-script build && docker build --tag cerealdevelopment/cerealbot:latest .",
    "docker-push": "docker push cerealdevelopment/cerealbot:latest",
    "docker-run": "docker run -d -v $(pwd)/config.json:/opt/cerealbot/config.json -v $(pwd)/data:/opt/cerealbot/data -v $(pwd)/logs:/opt/cerealbot/logs -e BOT_TOKEN=$BOT_TOKEN --name cerealbot cerealdevelopment/cerealbot:latest",
    "knex": "./node_modules/.bin/knex --knexfile src/data/knexfiles.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CerealDevelopment/CerealBot.git"
  },
  "keywords": [
    "Cereal",
    "Bot",
    "Discord"
  ],
  "author": "Cereal Development",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/CerealDevelopment/CerealBot/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged --ignore-path=.gitignore"
    }
  },
  "homepage": "https://github.com/CerealDevelopment/CerealBot#readme",
  "dependencies": {
    "@discordjs/opus": "^0.4.0",
    "@keyv/sqlite": "^2.0.2",
    "@types/node-cron": "^2.0.3",
    "discord.js": "^12.5.3",
    "keyv": "^4.0.3",
    "knex": "^0.95.6",
    "lodash": "^4.17.21",
    "node-cron": "^3.0.0",
    "node-fetch": "^2.6.1",
    "opusscript": "^0.0.8",
    "require": "^2.4.20",
    "sqlite3": "^4.1.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.0",
    "@babel/core": "^7.13.8",
    "@babel/node": "^7.13.0",
    "@babel/preset-env": "^7.13.8",
    "@babel/preset-typescript": "^7.13.0",
    "@types/jest": "^26.0.20",
    "@types/keyv": "^3.1.1",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.37",
    "@types/node-fetch": "^2.5.10",
    "fast-check": "^2.17.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "prettier": "2.2.1",
    "pretty-quick": "^3.1.0",
    "typescript": "^4.2.2"
  }
}
